{
    "name": "test-network-${ORG}",
    "version": "1.0.0",
    "client": {
        "organization": "${ORG}",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "${ORG}": {
            "mspid": "${MSP}",
            "peers": [
                "peer0.${ORG}.halonetwork.com"
            ],
            "certificateAuthorities": [
                "ca.${ORG}.halonetwork.com"
            ]
        }
    },
    "peers": {
        "peer0.${ORG}.halonetwork.com": {
            "url": "grpcs://${IP}:${P0PORT}",
            "tlsCACerts": {
                "pem": "${PEERPEM}"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.${ORG}.halonetwork.com",
                "hostnameOverride": "peer0.${ORG}.halonetwork.com"
            }
        }
    },
    "certificateAuthorities": {
        "ca.${ORG}.halonetwork.com": {
            "url": "https://${IP}:${CAPORT}",
            "caName": "ca-${ORG}",
            "tlsCACerts": {
                "pem": "${CAPEM}"
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}
